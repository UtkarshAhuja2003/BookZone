<%- include('../partials/header.ejs') %>
<%- include('../partials/navbar.ejs') %>
<body class="login">
    <main>
       <img src="https://i.ibb.co/txHV3T8/login.jpg" id="head"  alt="login">
        <h1>Login as student</h1>
        <form class="" action="/loginStudent" method="POST">
        <section>
            <div>
                <label for="">Enrollment Number</label>
                <input type="number" placeholder="10-digit" name="studentEnrollment" class="form-control">
                <i class="fa-solid fa-envelope registerrr fa-2x"></i>
            </div>
            <div>
                <label for="Password">Password</label>
                <input type="password" placeholder="Password" class="form-control" name="studentpassword">
                <i class="fa fa-lock registerrr fa-2x" aria-hidden="true"></i>
            </div>
            <div>
                <input type="submit" value="Sign in">
            </div>
            <a href="registerStudent">Create new account?</a>
        </section>
        </form>
    </main>


    <%- include('../partials/footer.ejs') %>
